<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.halokonsultan.mobile">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".HaloKonsultanApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HaloKonsultan">
        <activity android:name=".ui.consultant.ConsultantActivity"></activity>
        <activity android:name=".ui.profile.EditProfileActivity" />
        <activity
            android:name=".ui.profile.EditProfilActivity"
            android:exported="true" />
        <activity
            android:name=".ui.booking.BookingActivity"
            android:exported="true" />
        <activity
            android:name=".ui.splash.SplashActivity"
            android:theme="@style/splashScreenStyle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.chooselocation.ChooseLocationActivity" />
        <activity android:name=".ui.register.RegisterActivity" />
        <activity android:name=".ui.login.LoginActivity" />
        <activity android:name=".ui.landing.LandingActivity" />
        <activity android:name=".ui.main.MainActivity" />
    </application>

</manifest>