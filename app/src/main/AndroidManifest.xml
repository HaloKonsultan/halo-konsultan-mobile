<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.halokonsultan.mobile">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".HaloKonsultanApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HaloKonsultan">
        <activity
            android:name="com.halokonsultan.mobile.ui.profile.EditProfilActivity"
            android:exported="true" />
        <activity android:name="com.halokonsultan.mobile.ui.booking.BookingActivity"
            android:exported="true"/>
        <activity
            android:name="com.halokonsultan.mobile.ui.splash.SplashActivity"
            android:theme="@style/splashScreenStyle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.halokonsultan.mobile.ui.chooselocation.ChooseLocationActivity" />
        <activity android:name="com.halokonsultan.mobile.ui.register.RegisterActivity" />
        <activity android:name="com.halokonsultan.mobile.ui.login.LoginActivity" />
        <activity android:name="com.halokonsultan.mobile.ui.landing.LandingActivity" />
        <activity android:name="com.halokonsultan.mobile.ui.main.MainActivity" />
    </application>

</manifest>